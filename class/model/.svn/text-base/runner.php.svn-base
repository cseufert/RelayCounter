<?php

class modelRunner extends frameworkModel {
  const TABLE = "runner";
  
  static function openRow(&$o, $row) {
    $o->id = $row['id'];
    $o->chipcode = $row['chipcode'];
    $o->first = $row['first'];
    $o->last = $row['last'];
    $o->bibno = $row['bibno'];
    $o->teamid = $row['teamid'];
    $o->form = $row['form'];
    $o->percentage = $row['percentage'];
  }
  
}

?>
