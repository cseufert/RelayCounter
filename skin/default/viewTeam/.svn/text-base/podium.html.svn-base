<table width="100%" border="0" cellspacing="0" cellpadding="0">
  <tr>
    <td width="60%" height="160" align="center" valign="top"><img id="logo" src="../../img/logo.png" width="52" height="55" alt="247 relay challenge" /><span id="clock">__CLOCK__</span><div>Total Kilometers: <strong>__DISTANCE__ kms</strong></div></td>
    <td width="20%" align="center" valign="middle">
      <div class="medal"><img src="../../img/fastestlap_gold.png" /></div>
      <div class="title">Fastest Lap</div>
      <div class="team fastestlap">__S1NAME__</div>
      <div class="unit">__S1TIME__</div><div class="extension">s</div>
    </td>
    <td width="20%" align="center" valign="middle">
      <div class="medal"><img src="../../img/totalkms_gold.png" /></div>
      <div class="title">Current Form</div>
      <div class="team currentform">__S2NAME__</div>
      <div class="unit">__S2TIME__</div><div class="extension">laps in the last 30 minutes</div>
    </td>
  </tr>
  <tr>
    <td width="60%" height="160" align="center" valign="bottom">
    <div id="third"> <div class="position">3rd Place </div><br />__THIRD__</div>
    <div id="first"> <div class="position">1st Place </div><br />__FIRST__</div>   
	<div id="second"> <div class="position">2nd Place </div><br />__SECOND__</div>
 

    </td>
    <td class="panels" width="20%" align="center" valign="middle">
      <div class="medal"><img src="../../img/consistent_gold.png" /></div>
      <div class="title">Most Consistent</div>
      <div class="team mostconsistent">__S3NAME__</div>
      <div class="unit">__S3TIME__s</div><div class="extension"></div>
    </td>
    <td class="panels"width="20%" align="center" valign="middle">
      <div class="medal"><img src="../../img/teamplay_gold.png" /></div>
      <div class="title">Teamplay award</div>
      <div class="team teamplay">__S4NAME__</div>
      <div class="extension">Team members within </div><div class="unit">__S4TIME__</div><div class="extension"> lap(s)</div>
    </td>
  </tr>
</table>